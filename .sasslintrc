{
    "rules": {
        "no-transition-all": [ 0 ],
        "nesting-depth": [
            1,
            { "max-depth": 5 }
        ],
        "no-vendor-prefixes": [ 0 ],
        "property-sort-order": [ 0 ],
        "indentation": [
            1,
            { "size": 4 }
        ],
        "no-color-literals": [ 0 ],
        "class-name-format": [
            1,
            {
                "convention": "^bd-[a-z]"
            }
        ],
        "variable-name-format": [
            2,
            {
                "convention": "camelcase"
            }
        ],
        "url-quotes": [ 0 ]
    }
}
